  shootCooldown--;

if (hp &gt; MAXHP){
  if (extraLifesTotal - extraLifes &gt; 0){
    extraLifes++;
    hp -= 100;
  }else
    hp = 99;
}

if (hp &lt; 0){
  if (extraLifes &gt; 0){
    extraLifes--;
    hp += 100;
  }else
    hp = 0;
}

